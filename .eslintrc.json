{
	"extends": ["eslint:recommended"],
	"plugins": [],
	"parserOptions": {"ecmaVersion": "latest"},
	"env": {"browser": true},
	"globals": {},
	"rules": {
		"semi": ["error", "never"],
		"indent": ["error", "tab"],
		"no-console": "off",
		"no-unused-vars": "off",
		"singleQuote": "off",
		"no-undef": "off", // for not writing globals in every file
		"camelCase": "off", //private starts from _
		"spaced-comment": "off"
	}
}
